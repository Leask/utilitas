        "mysql2": "^3.13.0",
        "node-mailjet": "^6.0.8",
        "node-polyfill-webpack-plugin": "^4.1.0",
        "office-text-extractor": "^3.0.3",
        "ollama": "^0.5.14",
        "openai": "^4.87.3",
